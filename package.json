{
  "name": "semantic-release-cli",
  "version": "0.0.0-development",
  "description": "setup automated semver compliant package publishing",
  "author": "Christoph Witzko <npm@christophwitzko.com> (http://christophwitzko.com)",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "bin": {
    "semantic-release-cli": "./bin/semantic-release.js"
  },
  "bugs": {
    "url": "https://github.com/semantic-release/cli/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "base32": "0.0.6",
    "bluebird": "^3.4.6",
    "git-config-path": "^1.0.1",
    "github-url-from-git": "^1.4.0",
    "ini": "^1.3.4",
    "inquirer": "^3.3.0",
    "js-yaml": "^3.3.1",
    "lodash": "^4.16.4",
    "nopt": "^4.0.0",
    "npm": "^5.5.1",
    "npm-registry-client": "^8.5.0",
    "npmlog": "^4.0.0",
    "parse-git-config": "^1.1.1",
    "parse-github-repo-url": "^1.0.0",
    "request": "^2.58.0",
    "request-promise": "^4.1.1",
    "travis-ci": "^2.1.1",
    "update-notifier": "^2.3.0",
    "user-home": "^2.0.0",
    "validator": "^9.0.0"
  },
  "optionalDependencies": {
    "keytar": "^4.0.4"
  },
  "devDependencies": {
    "mkdirp": "^0.5.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "nyc": "^11.2.1",
    "rimraf": "^2.4.2",
    "semantic-release": "^8.0.0",
    "standard": "^10.0.3",
    "tap": "^10.0.0"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "files": [
    "bin",
    "src"
  ],
  "homepage": "https://github.com/semantic-release/cli",
  "keywords": [
    "author",
    "automation",
    "changelog",
    "module",
    "package",
    "publish",
    "release",
    "semver",
    "version"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/semantic-release/cli.git"
  },
  "scripts": {
    "coverage": "nyc report",
    "coverage:upload": "npm run coverage -- --reporter=lcovonly && coveralls < coverage/lcov.info",
    "prepublish": "npm run build",
    "cm": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "npm run test:style && npm run test:unit",
    "test:style": "standard",
    "test:unit": "nyc tap --no-cov test/specs/*.js"
  }
}
