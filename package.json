{
  "name": "semantic-release-cli",
  "description": "setup automated semver compliant package publishing",
  "version": "0.0.0-development",
  "author": "Christoph Witzko <npm@christophwitzko.com> (http://christophwitzko.com)",
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "bin": {
    "semantic-release-cli": "./bin/semantic-release.js"
  },
  "bugs": {
    "url": "https://github.com/semantic-release/cli/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "base32": "0.0.6",
    "bluebird": "^3.4.6",
    "clipboardy": "^1.2.2",
    "git-config-path": "^1.0.1",
    "github-url-from-git": "^1.4.0",
    "ini": "^1.3.4",
    "inquirer": "^4.0.0",
    "js-yaml": "^3.3.1",
    "lodash": "^4.16.4",
    "nopt": "^4.0.0",
    "npm": "^5.5.1",
    "npm-registry-client": "^8.5.0",
    "npmlog": "^4.0.0",
    "parse-git-config": "^1.1.1",
    "parse-github-repo-url": "^1.0.0",
    "request": "^2.58.0",
    "request-debug": "^0.2.0",
    "request-promise": "^4.1.1",
    "travis-ci": "^2.1.1",
    "update-notifier": "^2.3.0",
    "user-home": "^2.0.0",
    "validator": "^9.0.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "codecov": "^3.0.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "nyc": "^11.2.1",
    "prettier": "^1.10.0",
    "rimraf": "^2.4.2",
    "semantic-release": "^11.0.2",
    "xo": "^0.18.2"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "files": [
    "bin",
    "src"
  ],
  "homepage": "https://github.com/semantic-release/cli",
  "keywords": [
    "author",
    "automation",
    "changelog",
    "module",
    "package",
    "publish",
    "release",
    "semver",
    "version"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "json",
      "text",
      "html"
    ],
    "all": true
  },
  "optionalDependencies": {
    "keytar": "^4.0.4"
  },
  "preferGlobal": true,
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "bracketSpacing": false,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/semantic-release/cli.git"
  },
  "scripts": {
    "cm": "git-cz",
    "codecov": "codecov -f coverage/coverage-final.json",
    "lint": "xo",
    "pretest": "npm run lint",
    "semantic-release": "semantic-release",
    "test": "nyc ava -v"
  },
  "xo": {
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": 2
    }
  }
}
